::@save::
{
	if (!WARPERX){
	msgbox, 0, Coordinates, Warper Coords X is Missing!!!
	}
	else{
	}
	if (!WARPERY)
	{
	msgbox, 0, Coordinates, Warper Coords Y is Missing!!!
	}
	else{
	}
	if (!SELFX){
	msgbox, 0, Coordinates, SELF Coords X is Missing!!!
	}
	else{
	}
	if (!SELFY)
	{
	msgbox, 0, Coordinates, SELF Coords Y is Missing!!!
	}
	else{
	}
	if (!HEALERX){
	msgbox, 0, Coordinates, HEALER NPC X is Missing!!!
	}
	else{
	}
	if (!HEALERY){
	msgbox, 0, Coordinates, HEALER NPC Y is Missing!!!
	}
	else{
	}
	if (!TOOLX){
